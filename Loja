create database loja;
---------------------next step---------------------
use loja;
create table produtos(
id int not null auto_increment primary key,
produto varchar(100),
categoria varchar(100),
marca varchar(100),
modelo varchar(100),
preco double,
data date
);
---------------------next step---------------------
use loja;
